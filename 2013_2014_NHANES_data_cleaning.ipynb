   "outputs": [],
   "source": [
    "df_diet = pd.read_csv('diet.csv')\n",
    "df_examination = pd.read_csv('examination.csv')\n",
    "df_labs = pd.read_csv('labs.csv')\n",
    "df = pd.merge(df_diet, df_examination)\n",
    "df = pd.merge(df, df_labs)"
   ]
